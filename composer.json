{
    "name": "happyaccidents/dic-benchmark",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "orno/di": "dev-master",
        "pimple/pimple": "dev-master",
        "illuminate/container": "dev-master",
        "zendframework/zend-di": "dev-master",
        "symfony/dependency-injection": "dev-master",
        "aura/di": "dev-develop-2",
        "mnapoli/php-di": "dev-master",
        "league/di": "dev-master"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {"Benchmark": "src/"}
    },
    "scripts": {
        "benchmark": "php index.php > index.html",
        "gh-pages": "git checkout gh-pages && git merge master",
        "update": "composer run-script gh-pages && composer run-script benchmark",
        "publish": "git checkout master && git push && composer run-script update && git push"
    }
}
